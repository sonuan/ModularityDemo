<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.wusongyuan.mine"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:supportsRtl="true">
        <activity android:name="com.wusongyuan.mine.MineLoginActivity">
            <intent-filter>
                <data
                    android:host="mine"
                    android:path="/login"
                    android:scheme="app"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <action android:name="android.intent.action.VIEW"/>
            </intent-filter>
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
                <action android:name="com.wusongyuan.mine.login"/>
            </intent-filter>
        </activity>
    </application>

</manifest>